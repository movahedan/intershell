{
	"private": false,
	"name": "intershell",
	"version": "0.3.1",
	"description": "Bunâ€‘first CLI toolkit for developer workflow automation in monorepos and monoliths.",
	"packageManager": "bun@1.3.1",
	"engines": {
		"bun": ">=1.0.0"
	},
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"intershell": "./dist/index.js"
	},
	"exports": {
		"./core": "./dist/core/index.js",
		".": "./dist/index.js",
		"./entities": "./dist/entities/index.js"
	},
	"oclif": {
		"bin": "intershell",
		"dirname": "intershell",
		"commands": "./dist/commands",
		"plugins": [
			"@oclif/plugin-help",
			"@oclif/plugin-version"
		]
	},
	"files": [
		"dist",
		"src",
		"README.md"
	],
	"scripts": {
		"check:types": "tsc --noEmit",
		"build": "rm -rf dist && tsc -p tsconfig.build.json && echo '#!/usr/bin/env bun' > dist/index.js.tmp && tail -n +2 dist/index.js >> dist/index.js.tmp && mv dist/index.js.tmp dist/index.js",
		"postbuild": "bun run dist/index.js update-package-json",
		"check": "biome check .",
		"check:fix": "biome check . --write",
		"test": "bun test",
		"check:quick": "bun run check:fix && bun run check:types && bun run test && bun run build",
		"commit:check": "bun run ./src/commands/commit-check.ts"
	},
	"devDependencies": {
		"@oclif/core": "4.5.4",
		"@oclif/dev-cli": "1.26.10",
		"@oclif/plugin-help": "6.2.33",
		"@oclif/plugin-version": "2.2.33",
		"bun-types": "^1.3.3"
	},
	"peerDependencies": {
		"@biomejs/biome": "2.1.2",
		"lefthook": "1.12.4",
		"turbo": "2.5.8",
		"typescript": "5.9.3"
	},
	"keywords": [
		"cli",
		"interactive",
		"terminal",
		"framework",
		"pages",
		"navigation",
		"state-management",
		"command-parser",
		"plugin-system",
		"entities",
		"commit-management",
		"branch-validation"
	]
}
